{
  "name": "Meal-planner-back",
  "version": "1.0.0",
  "main": "src/main.ts",
  "repository": "git@github.com:GarnierLenny/Meal-planner-back",
  "author": "Lenny Garnier <lenny.garnier00@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "yarn run build && node dist/src/main.js",
    "start:dev": "npx prisma generate && tsx watch src/main.ts",
    "start:debug": "tsx --inspect src/main.ts",
    "start:prod": "node dist/src/main.js",
    "build": "npx prisma generate && tsc",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "TEST_SERIAL=1 jest tests/e2e --runInBand",
    "test:e2e:health": "TEST_SERIAL=1 jest tests/e2e/health.e2e.test.ts --runInBand",
    "test:e2e:auth": "TEST_SERIAL=1 jest tests/e2e/auth.e2e.test.ts --runInBand",
    "test:e2e:recipes": "TEST_SERIAL=1 jest tests/e2e/recipes.e2e.test.ts --runInBand",
    "test:e2e:meal-plans": "TEST_SERIAL=1 jest tests/e2e/meal-plans.e2e.test.ts --runInBand",
    "test:e2e:grocery": "TEST_SERIAL=1 jest tests/e2e/grocery-list.e2e.test.ts --runInBand",
    "test:e2e:user-health": "TEST_SERIAL=1 jest tests/e2e/user-health.e2e.test.ts --runInBand",
    "test:e2e:chat": "TEST_SERIAL=1 jest tests/e2e/chat.e2e.test.ts --runInBand",
    "test:e2e:verbose": "TEST_SERIAL=1 jest tests/e2e --runInBand --verbose",
    "test:e2e:watch": "TEST_SERIAL=1 jest tests/e2e --runInBand --watch",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:pull": "npx prisma db pull",
    "db:migrate": "npx prisma migrate dev",
    "db:migrate:deploy": "npx prisma migrate deploy",
    "db:migrate:reset": "npx prisma migrate reset",
    "db:seed": "npx prisma db seed",
    "db:studio": "npx prisma studio",
    "db:validate": "npx prisma validate",
    "db:format": "npx prisma format",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:status": "npx supabase status",
    "supabase:reset": "npx supabase db reset",
    "supabase:diff": "npx supabase db diff",
    "supabase:link": "npx supabase link",
    "supabase:gen-types": "npx supabase gen types typescript --local > src/types/supabase.ts",
    "dev:db": "npm run supabase:start && npm run db:push",
    "dev:reset": "npm run supabase:reset && npm run db:push",
    "dev:fresh": "npm run supabase:stop && npm run supabase:start && npm run db:push",
    "prod:schema-fix": "npx prisma db execute --file ./fix-production-schema.sql --schema ./prisma/schema.prisma",
    "prod:add-fks": "npx prisma db execute --file ./add-missing-fks.sql --schema ./prisma/schema.prisma",
    "prod:introspect": "npx prisma db pull",
    "prod:diff": "npx prisma migrate diff --from-empty --to-schema-datamodel prisma/schema.prisma --script",
    "prod:update": "npm run prod:schema-fix && npm run prod:add-fks && npm run db:generate"
  },
  "dependencies": {
    "@google/genai": "^1.29.1",
    "@nestjs/common": "^11.1.8",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.8",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.8",
    "@nestjs/swagger": "^11.2.1",
    "@prisma/client": "^6.15.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.57.0",
    "@tsoa/runtime": "^6.6.0",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "nestjs-zod": "^5.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^6.15.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "tsoa": "^6.6.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.1.9",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "jest": "^30.1.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.5",
    "typescript": "^5.8.3",
    "uuid": "^13.0.0"
  },
  "engines": {
    "node": ">=20"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
